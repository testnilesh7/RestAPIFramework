<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="API Regression Test Suit" verbose="4">
<listeners>
		<listener class-name="com.aventstack.chaintest.plugins.ChainTestListener" />
</listeners>
	<test name="Go Rest API Test - Get User">
		<classes>
			<class name="com.qa.api.gorest.tests.GetUserTest" />
		</classes>
	</test>

	<test name="Go Rest API Test - Create User ">
		<classes>
			<class name="com.qa.api.gorest.tests.CreateUserTest" />
		</classes>
	</test>

	<test name="Go Rest API Test - Update User">
		<classes>
			<class name="com.qa.api.gorest.tests.UpdateUserTest" />
		</classes>
	</test>

	<test name="Go Rest API Test - Delete User">
		<classes>
			<class name="com.qa.api.gorest.tests.DeleteUserTest" />
		</classes>
	</test>

	<test name="Product API Test">
		<classes>
			<class name="com.qa.api.products.tests.ProductAPITest" />
		</classes>
	</test>

	<test name="Contact API - Create New Contact Test">
		<classes>
			<class name="com.qa.api.contacts.tests.CreateContactsTest" />
		</classes>
	</test>

	<test name="Contact API - Get All Contacts Test">
		<classes>
			<class name="com.qa.api.contacts.tests.GetContactsTest" />
		</classes>
	</test>

	<test name="Booking API -Create New Booking Test">
		<classes>
			<class name="com.qa.api.booking.tests.CreateBookingTest" />
		</classes>
	</test>


</suite>